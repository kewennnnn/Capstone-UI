{
  "name": "LivUltra",
  "productName": "LivUltra",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . LivUltra --overwrite --asar=true --platform=win32 --arch=ia32 --icon=media/logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"LivUltra\""
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^19.0.4",
    "electron-packager": "^15.5.2"
  },
  "dependencies": {
    "electron-localstorage": "^1.0.5",
    "electron-reload": "^2.0.0-alpha.1"
  }
}
